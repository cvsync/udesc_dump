item	LOCAL DESIGNATOR_MAX	5	Designator Maximum
item	LOCAL STRING_INDEX	6	String Index
item	LOCAL STRING_MIN	7	String Minimum
item	LOCAL STRING_MAX	8	String Maximum
item	LOCAL DELIMITER		9	Delimiter
 
